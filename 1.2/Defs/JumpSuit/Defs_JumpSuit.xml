      <wornGraphicPath>Things/Item/JPJumpSuit/JPJumpSuit</wornGraphicPath>
      <layers>
        <li>OnSkin</li>
      </layers>
      <bodyPartGroups>
        <li>Neck</li>
        <li>Torso</li>
        <li>Shoulders</li>
        <li>Arms</li>
        <li>Legs</li>
        <li>Feet</li>
        <li>Hands</li>
      </bodyPartGroups>
    </apparel>
    <recipeMaker>
      <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
      <workSkill>Crafting</workSkill>
      <effectWorking>Tailor</effectWorking>
      <soundWorking>Recipe_Tailor</soundWorking>
      <unfinishedThingDef>UnfinishedApparel</unfinishedThingDef>
      <researchPrerequisite>PowArmJetPack</researchPrerequisite>
      <recipeUsers>
        <li>ElectricTailoringBench</li>
        <li>HandTailoringBench</li>
      </recipeUsers>
      <skillRequirements>
        <Crafting>7</Crafting>
      </skillRequirements>
