    <equippedStatOffsets>
      <ToxicSensitivity>-0.1</ToxicSensitivity>
    </equippedStatOffsets>
    <costStuffCount>100</costStuffCount>
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
